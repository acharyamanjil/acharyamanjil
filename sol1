//Given three numbers X,Y &Z. Write a function that finds the greatest among the numbers.
#include <stdio.h>
 
void main()
{
    int num1, num2, num3;
 
    printf("Enter the values of num1, num2 and num3\n");
    scanf("%d %d %d", &num1, &num2, &num3);
    printf("First Number = %d\tSecond Number = %d\tThird Number = %d\n", num1, num2, num3);
    if (num1 > num2)
    {
        if (num1 > num3)
        {
            printf("First number is the greatest among three. Value Is :%d\n",num1);
        }
        else
        {
            printf("Third number is the greatest among three. Value Is :%d\n",num3);
        }
    }
    else if (num2 > num3)
        printf("Second number is the greatest among three. Value Is :%d\n",num2);
    else
        printf("Third number is the greatest among three. Value Is :%d\n",num3);
}
